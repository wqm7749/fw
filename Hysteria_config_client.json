
{
  "inbounds": [
    {
      "type": "mixed",
      "listen": "::",
      "listen_port": 10000
    }
  ],
  "outbounds": [
    {
      "tag": "proxy",
      "type": "hysteria",
      "server": "",
      "server_port": 443,
      "up_mbps": 20,
      "down_mbps": 100,
      "tcp_fast_open": true,
      "domain_strategy": "prefer_ipv4",
      "fallback_delay": "300ms",
      "recv_window_conn": 0,
      "recv_window": 0,
      "disable_mtu_discovery": false,
      "auth_str": "password",
      "tls":
      {
        "enabled": true,
        "server_name": "www",
        "disable_sni": true,
        "insecure": true,
        "alpn": [
          "h3"
        ]
      }
    }
  ]
}

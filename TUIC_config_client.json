
{
  "inbounds": [
    {
      "type": "mixed",
      "listen": "::",
      "listen_port": 10000
    }
  ],
  "outbounds": [
    {
      "type": "tuic",
      "server": "*",
      "server_port": 443,
      "uuid": "*",
      "password": "*",
      "congestion_control": "bbr",
      "udp_relay_mode": "native",
      "heartbeat": "10s",
      "connect_timeout": "5s",
      "tcp_fast_open": true,      
      "udp_over_stream": false,
      "tcp_multi_path": false,
      "udp_fragment": false,
      "domain_strategy": "prefer_ipv4",
      "fallback_delay": "300ms",
      "tls":
      {
        "enabled": true,
        "insecure": true,
        "disable_sni": false,
        "server_name": "*",
        "alpn": [
          "h3"
        ]
      }
    }
  ]
}
